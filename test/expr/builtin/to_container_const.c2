// @warnings{no-unused}
module test;

type List struct {
    List* next;
    List* prev;
}

type Struct struct {
    i32 x;
    List list;
}

func void test1() {
    Struct s1 = { 1 }
    const List* l = &s1.list;
    u32 num = to_container(Struct, list, l); // @error{incompatible pointer to integer conversion const (struct)test.Struct* to u32}
}

func void test2() {
    Struct s1 = { 1 }
    const List* l = &s1.list;
    Struct* sp = to_container(Struct, list, l); // @error{pointer conversion discards const qualifier}
}


// @warnings{no-unused}
module test;

type Struct struct {
    i32 x;
}

type Enum enum u32 {
    A, B
}

type EnumB enum u8 {
    C, D
}

type Func func void(Enum);

func void test1(Enum e) {
    bool a = cast<bool>(e);
    u8 b = cast<u8>(e);
    u32 c = cast<u32>(e);
    f32 d = cast<f32>(e);
    u32* f = cast<u32*>(e);   // @error{invalid cast from '(enum)test.Enum' to 'u32*'}
}

func void test2(Enum e) {
    Struct* g = cast<Struct*>(e);   // @error{invalid cast from '(enum)test.Enum' to '(struct)test.Struct*'}
}

func void test3(Enum e) {
    EnumB h = cast<EnumB>(e);
    Func i = cast<Func>(e);         // @error{invalid cast from '(enum)test.Enum' to 'void ((enum)test.Enum)'}
}


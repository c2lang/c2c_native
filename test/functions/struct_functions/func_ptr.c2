// @warnings{no-unused}
module test;

type Type struct {
    i32 x;
}

func void Type.init(Type* t) {}


type Func func void(Type*);

func void myfunc() {
    Type t;
    Func f1 = Type.init;  // static struct-function usage
    Func f2 = t.init;     // @error{non-static use of struct-function is only allowed as function call}
}


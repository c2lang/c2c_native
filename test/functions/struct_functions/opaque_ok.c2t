module foo;

public type Type struct @(opaque) {
    i32 x;
}

public fn Type* Type.create() {
    Type* t = nil;
    return t;
}

fn void Type.internal(Type* t) {
    t.x = 1;
}

public fn void Type.init(Type* t) {
    t.internal();
}


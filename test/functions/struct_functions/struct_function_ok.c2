// @warnings{no-unused}
module test;

type Type struct {
    i32 member;
}

fn void Type.init(Type* t) {
    t.member = 0;
}

fn i32 Type.getMember(const Type* t) {
    return t.member;
}

fn void Type.add(Type* t, i32 n) {
    t.member += n;
}

fn void myfunc() {
    Type t;
    t.init();
    i32 m = t.getMember();
    t.add(1);

    Type.init(&t);
    Type.add(&t, 2);
    m = Type.getMember(&t);
}

